program main
  use const_base_m
  use svglib_m
  implicit none

  type(svglib_fig_t) :: fig01, fig02, fig03

  call const_base__check

  call fig01.open( "test.svg", width=300, height=400 )
  call fig01.line( 10.0, 10.0, 100.0, 100.0, stroke="black", stroke_width=1.0 )
  call fig02.open( "test02.svg", width=300, height=400 )
  call fig03.open( "test03.svg", width=300, height=400 )
  call fig03.close
  call fig02.close
  call fig01.close
end program main
